{
  "name": "htsnexus_index_merger",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "inputSpec": [
    {
      "name": "htsnexus_index",
      "class": "array:file",
      "type": "htsnexus_index"
    },
    {
      "name": "output_name",
      "class": "string",
      "default": "htsnexus_index"
    }
  ],
  "outputSpec": [
    {
      "name": "index_db",
      "class": "file",
      "type": "htsnexus_index"
    }
  ],
  "runSpec": {
    "interpreter": "bash",
    "file": "src/code.sh",
    "execDepends": [
      {"name": "sqlite3"},
      {"name": "liblz4-tool"}
    ],
    "systemRequirements": {
      "main": {
        "instanceType": "mem1_ssd1_x2"
      }
    },
    "distribution": "Ubuntu",
    "release": "14.04"
  },
  "authorizedUsers": []
}
